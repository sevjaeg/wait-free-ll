#! /bin/bash

#SBATCH -p q_student
#SBATCH -N 1                 
#SBATCH -c 32
#SBATCH --cpu-freq=High
#SBATCH --time=5:00
#SBATCH --output=wfll_1a.out

NLIST=(100 200 500 1000)
PLIST=(1 2 4 8 16 24 32)
NREP=30
BINARY="bin/wfll"

for n in "${NLIST[@]}"
do
  for p in "${PLIST[@]}"
  do
    for nrep in `seq 1 ${NREP}`
    do
      ${BINARY} -p $p -n $n -i $n
    done    
  done  
done